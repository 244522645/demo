<%@ page language="java" contentType="text/html; charset=utf-8"	pageEncoding="utf-8"%><%	String path = request.getContextPath();			String basePath = request.getScheme() + "://" + request.getServerName() + ":" + request.getServerPort()					+ path + "/";%><!DOCTYPE html><!--[if IE 8]> <html lang="en" class="ie8"> <![endif]--><!--[if IE 9]> <html lang="en" class="ie9"> <![endif]--><!--[if !IE]><!--><html lang="en"><!--<![endif]--><!-- BEGIN HEAD --><head><meta charset="utf-8" /><title>Metronic | Login Page</title><meta content="width=device-width, initial-scale=1.0" name="viewport" /><meta content="" name="description" /><meta content="" name="author" /><!-- BEGIN GLOBAL MANDATORY STYLES --><link href="<%=path%>/media/css/bootstrap.min.css" rel="stylesheet"	type="text/css" /><link href="<%=path%>/media/css/bootstrap-responsive.min.css"	rel="stylesheet" type="text/css" /><link href="<%=path%>/media/css/DT_bootstrap.css" rel="stylesheet"	type="text/css" /><link href="<%=path%>/media/css/font-awesome.min.css" rel="stylesheet"	type="text/css" /><link href="<%=path%>/media/css/style-metro.css" rel="stylesheet"	type="text/css" /><link href="<%=path%>/media/css/style.css" rel="stylesheet"	type="text/css" /><link href="<%=path%>/media/css/daterangepicker-bs3.css"	rel="stylesheet" type="text/css" /><!-- END GLOBAL MANDATORY STYLES --><!-- BEGIN PAGE LEVEL STYLES --><link href="<%=path%>/media/css/operations.css" rel="stylesheet"	type="text/css" /><!-- END PAGE LEVEL STYLES --><link href="<%=path%>/media/css/sweetalert.css" rel="stylesheet"	type="text/css" /></head><!-- END HEAD --><!-- BEGIN BODY --><body class="login">	<!-- BEGIN LOGIN -->	<div style="margin-top: 10px;" align="center" id="description">		<ul class="ver-inline-menu tabbable margin-bottom-5">			<li style="text-align: center;" id="time">				<button type="submit" class="btn" style="width: 20%;">今天</button>				<button type="submit" class="btn" style="width: 20%;">昨天</button>				<button type="submit" class="btn" style="width: 20%;">本月</button>				<button type="submit" class="btn" style="width: 20%;">上月</button>			</li>		</ul>		<div class="content" align="center"			style="margin-top: 10px; margin-bottom: 10px; padding-bottom: 0px;">			<div class="input-prepend input-group">				<span class="add-on input-group-addon" style="height: 20px;">开始时间:</span><input type="text" name="starttime"					id="starttime" class="form-control" value="2016-03-08" />			</div>		</div>		<div class="content" align="center"			style="margin-top: 10px; margin-bottom: 10px; padding-bottom: 0px;">			<div class="input-prepend input-group">				<span class="add-on input-group-addon" style="height: 20px;">结束时间:</span><input type="text" name="endtime"					id="endtime" class="form-control" value="2016-03-08" />			</div>		</div>		<div class="form-actions"			style="background-color: #fff; border-top: 0px; margin-top: 0px; margin-bottom: 0px; padding-top: 0px;">			<button type="button" id="find-btn" class="btn red"				style="width: 100%; background-color: #f9625b !important;">				查询 <i class="m-icon-swapright m-icon-white"></i>			</button>		</div>		<table class="table table-hover">			<thead>				<tr>					<th style="text-align: center;">单价（元）</th>					<th style="text-align: center;">数量（张）</th>					<th style="text-align: center;">总价（元）</th>				</tr>			</thead>			<tbody id="tabletbody">			</tbody>		</table>		<span>校验码：<span id="serialnumber"></span></span>		<div class="form-actions"			style="background-color: #fff; border-top: 0px; margin-top: 0px; margin-bottom: 0px; padding-top: 0px;">			<button type="button" id="register-submit-btn" class="btn red"				style="width: 100%; background-color: #f9625b !important;" onclick="printTicket()">				打印小票 <i class="m-icon-swapright m-icon-white"></i>			</button>		</div>		<div class="form-actions"			style="background-color: #fff; border-top: 0px; margin-top: 0px; margin-bottom: 0px; padding-top: 0px;">			<button type="button" id="more-btn" class="btn red"				style="width: 100%; background-color: #f9625b !important;">				查询明细 <i class="m-icon-swapright m-icon-white"></i>			</button>		</div>	</div>	<div style="margin-top: 10px;" align="center" id="more_description">		<table class="table table-hover">			<thead>				<tr>					<th style="text-align: center;width: 30%;">卡号/流水号</th>					<th style="text-align: center;width: 36%;">时间</th>					<th style="text-align: center;width: 15%;">张数</th>					<th style="text-align: center;width: 15%;">单价</th>									</tr>			</thead>			<tbody id="more_tabletbody">			</tbody>		</table>		<div class="form-actions"			style="background-color: #fff; border-top: 0px; margin-top: 0px; margin-bottom: 0px; padding-top: 0px;">			<button type="button" id="more-findmore" class="btn red"				style="width: 100%; background-color: #f9625b !important;">				加载更多 <i class="m-icon-swapright m-icon-white"></i>			</button>		</div>		<div class="form-actions"			style="background-color: #fff; border-top: 0px; margin-top: 0px; margin-bottom: 0px; padding-top: 0px;">			<button type="button" id="more-rollback" class="btn red"				style="width: 100%; background-color: #f9625b !important;">				返回上级 <i class="m-icon-swapright m-icon-white"></i>			</button>		</div>	</div>	<!-- END LOGIN -->	<!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->	<!-- BEGIN CORE PLUGINS -->	<script src="<%=path%>/media/js/jquery-1.10.1.min.js"		type="text/javascript"></script>	<!-- IMPORTANT! Load jquery-ui-1.10.1.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->	<script src="<%=path%>/media/js/jquery-ui-1.10.1.custom.min.js"		type="text/javascript"></script>	<script src="<%=path%>/media/js/bootstrap.min.js"		type="text/javascript"></script>	<!--[if lt IE 9]>	<script src="<%=path%>/media/js/excanvas.min.js"></script>	<script src="<%=path%>/media/js/respond.min.js"></script>  	<![endif]-->		<script src="<%=path%>/media/js/moment.js" type="text/javascript"></script>	<script src="<%=path%>/media/js/daterangepicker.js"		type="text/javascript"></script>	<!-- END CORE PLUGINS -->	<!-- BEGIN PAGE LEVEL PLUGINS -->	<!-- END PAGE LEVEL PLUGINS -->	<!-- BEGIN PAGE LEVEL SCRIPTS -->	<script src="<%=path%>/media/js/operations.js?_=<%=System.currentTimeMillis()%>" type="text/javascript"></script>	<script src="<%=path%>/media/js/sweetalert.min.js"		type="text/javascript"></script>	<!-- END PAGE LEVEL SCRIPTS -->	<script>		jQuery(document).ready(function() {			$('#starttime').daterangepicker({				singleDatePicker : true			}, function(start, end, label) {				console.log(start.toISOString(), end.toISOString(), label);			});			$('#endtime').daterangepicker({				singleDatePicker : true			}, function(start, end, label) {				console.log(start.toISOString(), end.toISOString(), label);			});						Login.init();		});	</script>	<script type="text/javascript">		var mobileType = '${sessionScope.currentUser.mobileType}';	function printTicket() {		if(result==null||result==""){			swal("您选择的时间段没有消费");			return;		}		var starttime = $("#starttime").val();		var endtime = $("#endtime").val();		var title='${sessionScope.cinemaName.name}';		var printText = "结算单\n"+'终端号:'+'${sessionScope.cinemaName.posid}'+'\n'+"流水号:"+result[1][0].serialnumber+"\n"+"校验码:"+result[1][0].baoxian+"\n"+"---------------------\n"+"起始时间："+starttime+"\n"+"截止时间："+endtime+"\n"+"单价(元)    数量(张)    总价(元)\n";		var cinemapricecount=0;		var posordercount=0;				$.each(result[0],function(i,p){			printText=printText+p.cinemaprice+"             "+p.num+"           "+(p.cinemaprice*p.num)+"\n";				cinemapricecount=cinemapricecount+p.num;			posordercount=posordercount+(p.cinemaprice*p.num);		});		printText=printText+"合计           "+cinemapricecount+"           "+posordercount+"\n"+"---------------------\n"+"看购电影感谢您的支持\n"+"看购网 www.kanggou.cn\n";						if (mobileType == 'ios') {				kangouprint("1B/33/10/1D/21/11",title);			kangouprint("1B/33/25/1D/21/00",printText);		} else {							kangou.checkPrinter();			kangou.print("29/33/17/27/51/15",'${sessionScope.cinemaName.name}'+'\n');			kangou.print("27/97/0/27/51/15","结算单\n");			kangou.print("27/97/0/27/51/15",'终端号:'+'${sessionScope.cinemaName.posid}'+'\n');			kangou.print("27/97/0/27/51/15","流水号:"+result[1][0].serialnumber+"\n");			kangou.print("27/97/0/27/51/15","校验码:"+result[1][0].baoxian+"\n");			kangou.print("27/97/0/27/51/15","---------------------\n");			kangou.print("27/97/0/27/51/15","起始时间："+starttime+"\n");			kangou.print("27/97/0/27/51/15","截止时间："+endtime+"\n");			kangou.print("27/97/0/27/51/15","单价(元)    数量(张)    总价(元)\n");			var cinemapricecount=0;			var posordercount=0;			$.each(result[0],function(i,p){				kangou.print("27/97/0/27/51/15",p.cinemaprice+"             "+p.num+"           "+(p.cinemaprice*p.num)+"\n");					cinemapricecount=cinemapricecount+p.num;				posordercount=posordercount+(p.cinemaprice*p.num);			});			kangou.print("27/97/0/27/51/15","合计           "+cinemapricecount+"           "+posordercount+"\n");			kangou.print("27/97/0/27/51/15","---------------------\n");			kangou.print("27/97/0/27/51/15","看购电影感谢您的支持\n");			kangou.print("27/97/0/27/51/15","看购网 www.kanggou.cn\n");			kangou.print("27/100/3","");								}	}		</script>	<!-- END JAVASCRIPTS --></body><!-- END BODY --></html>